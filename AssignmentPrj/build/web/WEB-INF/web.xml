<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ProductionPlanListController</servlet-name>
        <servlet-class>controller.productionplan.ProductionPlanListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanCreateController</servlet-name>
        <servlet-class>controller.productionplan.ProductionPlanCreateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.accesscontrol.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanCampaignListController</servlet-name>
        <servlet-class>controller.productionplancampaign.ProductionPlanCampaignListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanCampaignDetailController</servlet-name>
        <servlet-class>controller.productionplancampaign.ProductionPlanCampaignDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SchedualCampaignCreateController</servlet-name>
        <servlet-class>controller.SchedualCampaign.SchedualCampaignCreateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.accesscontrol.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanDeleteController</servlet-name>
        <servlet-class>controller.productionplan.ProductionPlanDeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanRestoreController</servlet-name>
        <servlet-class>controller.productionplan.ProductionPlanRestoreController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanUpdateController</servlet-name>
        <servlet-class>controller.productionplan.ProductionPlanUpdateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>controller.accesscontrol.Home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductionPlanListController</servlet-name>
        <url-pattern>/productionplan/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanCreateController</servlet-name>
        <url-pattern>/productionplan/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanCampaignListController</servlet-name>
        <url-pattern>/productionplancampaign/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanCampaignDetailController</servlet-name>
        <url-pattern>/productionplancampaign/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SchedualCampaignCreateController</servlet-name>
        <url-pattern>/schedualcampaign/create</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanDeleteController</servlet-name>
        <url-pattern>/productionplan/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanRestoreController</servlet-name>
        <url-pattern>/productionplan/restore</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanUpdateController</servlet-name>
        <url-pattern>/productionplan/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>25</session-timeout>
    </session-config>
</web-app>
